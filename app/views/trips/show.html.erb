<div class="image-wrapper" style="background-image:
linear-gradient(-225deg, rgba(186,210,213,0.4) 0%, rgba(0,36,61,0.4) 50%), url(https://res.cloudinary.com/druzius/image/upload/v1543757575/architecture-bridges-cathedral-208213.jpg);">

<div class="container">
  <div class="row">
    <div class="show-category-title">
      <h2>You are traveling to <%= @trip.city %> </h2>
      <h3>Check the below boxes in order to customize your trip</h3>
    </div>
    <div class="show-category-card-sect">
      <div class="col-md-12 col-lg-8">
        <% @foursquare_categories.each do |cat| %>
        <%= link_to new_trip_venue_path(@trip, section: cat.name) do  %>
        <div class="show-category-card">
          <div class="show-category-card-caption"><%= cat.name %></div>
          <%= image_tag cat.img_url, class: "img-fluid" %>
        </div>
        <% end %>
        <% end %>
      </div>
    </div>
  </div>
</div>



</div>
